Algoritmo CalcularParteCorrespondiente
	Escribir "Dados una cantidad y un por centaje calcular la parte correspondiente.";
	
	Escribir "Introduzca la cantidad: ";
	Leer cantidad;
	
	Escribir "Introduzca el porcentaje: ";
	Leer porcentaje;
	
	parte_correspondiente = (cantidad * porcentaje) / 100;
	
	Escribir parte_correspondiente;
FinAlgoritmo
